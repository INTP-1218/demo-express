{
  "name": "demo-express",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "private": true,
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.6.2",
    "npm": "please use pnpm"
  },
  "scripts": {
    "start": "node src/app.js",
    "dev-start": "pm2 start ecosystem.config.js",
    "dev-stop": "pm2 stop demo-app",
    "dev": "nodemon src/app.js",
    "test": "cross-env IS_TESTING=true mocha --require tests/fixtures.test.js --require tests/teardown.test.js ./*/test/*.test.js --reporter mochawesome -options reportDir=testReports,reportFilename=automatedTestReports --timeout 250000 --exit --bail",
    "lint": "eslint .",
    "lint:fix": "eslint .  --fix",
    "migrate-up": "migrate-mongo up",
    "migrate-down": "migrate-mongo down",
    "migrate-status": "migrate-mongo status",
    "logs": "git log --pretty='%C(Yellow)%h  %C(reset)%ad (%C(Green)%cr%C(reset))%x09 %C(Cyan)%an: %C(reset)%s'",
    "prepare": "husky install",
    "audit:report": "pnpm audit --json | npm-audit-html --output vulnerablities.html",
    "audit:": "pnpm audit --json | npm-audit-html --output vulnerablities.html --fatal-exit-code",
    "clean": "rm pnpm-lock.yaml && rm -rf node_modules",
    "docker-up": "docker-compose up",
    "docker-down": "docker-compose down",
    "docker-clean-containers": "docker rm $(docker ps -aq)",
    "docker-clean-images": "docker rmi $(docker images -q)",
    "precommit:lint": "pnpm run lint:fix && git add ."
  },
  "keywords": [
    "express",
    "template",
    "mongodb",
    "proect-template"
  ],
  "homepage": "",
  "url": "",
  "email": "",
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "file-stream-rotator": "^1.0.0",
    "helmet": "^7.0.0",
    "ip": "^1.1.8",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.3.0",
    "newrelic": "^10.3.0",
    "node-cron": "^3.0.2",
    "pino": "^8.14.1",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "generate-password": "^1.7.0",
    "husky": "^8.0.3",
    "migrate-mongo": "^10.0.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^2.0.22",
    "pm2": "^5.3.0",
    "prettier": "^2.8.8"
  }
}
